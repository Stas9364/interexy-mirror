{
	"singleQuote": true,
	"jsxSingleQuote":true,
  "arrowParens": "avoid",
  "semi": true,
  "bracketSpacing": true,
  "trailingComma": "all",
  "printWidth": 90,
  "tabWidth": 2,
  "bracketSameLine": false,
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["prettier-plugin-tailwindcss"]
}